<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.CustomerParserController">
   <children>
      <TableView fx:id="headerTable" layoutX="43.0" layoutY="32.0" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="headerColumn" prefWidth="199.0" text="Header" />
            <TableColumn prefWidth="199.0" text="Header" />
        </columns>
      </TableView>
      <Label layoutX="300.0" layoutY="32.0" prefHeight="102.0" prefWidth="138.0" text="Please add the corresponding headings to match the data in the CSV file and select a valid CSV location" wrapText="true" />
      <TextField fx:id="headerField" layoutX="43.0" layoutY="243.0" />
      <Button fx:id="addHeader" layoutX="200.0" layoutY="243.0" mnemonicParsing="false" onAction="#newHeader" text="+" />
      <Button fx:id="chooseFile" layoutX="300.0" layoutY="141.0" mnemonicParsing="false" onAction="#chooseFile" text="Choose File" />
      <Button fx:id="deleteHeader" layoutX="43.0" layoutY="278.0" mnemonicParsing="false" onAction="#deleteHeader" text="Delete Selection" />
      <Label fx:id="selectedFileLabel" layoutX="300.0" layoutY="183.0" text="Label" />
      <Label fx:id="error" layoutX="43.0" layoutY="354.0" text="Label" />
      <Button fx:id="back" layoutX="534.0" layoutY="350.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
   </children>
</AnchorPane>
