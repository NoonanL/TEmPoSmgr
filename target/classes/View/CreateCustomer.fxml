<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="card" stylesheets="@test.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.CreateCustomerController">
   <children>
      <Label layoutX="41.0" layoutY="88.0" text="First Name:" />
      <Label layoutX="41.0" layoutY="154.0" text="Surname:" />
      <TextField fx:id="firstnameField" layoutX="123.0" layoutY="79.0" styleClass="text-field" />
      <Label layoutX="14.0" layoutY="14.0" styleClass="card-title" text="Create New Customer" />
      <TextField fx:id="surnameField" layoutX="123.0" layoutY="145.0" styleClass="text-field" />
      <Button fx:id="back" layoutX="734.0" layoutY="561.0" mnemonicParsing="false" onAction="#backClicked" styleClass="button-raised" text="Back" />
      <Button fx:id="submit" layoutX="241.0" layoutY="202.0" mnemonicParsing="false" onAction="#createNewCustomer" styleClass="button-raised" text="Create" />
      <Label fx:id="error" layoutX="196.0" layoutY="573.0" prefHeight="20.0" prefWidth="411.0" />
   </children>
</AnchorPane>
