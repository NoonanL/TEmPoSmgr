<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="267.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.LoginController">
   <children>
      <TextField fx:id="username" layoutX="76.0" layoutY="96.0" />
      <TextField fx:id="password" layoutX="76.0" layoutY="147.0" />
      <Button fx:id="loginButton" layoutX="76.0" layoutY="177.0" mnemonicParsing="false" onAction="#isLoginClicked" prefHeight="25.0" prefWidth="101.0" text="Login" />
      <Button fx:id="quitButton" layoutX="186.0" layoutY="177.0" mnemonicParsing="false" onAction="#handleQuit" text="Quit" />
      <Label layoutX="13.0" layoutY="14.0" prefHeight="37.0" prefWidth="270.0" text="TEmPoS Manager Login">
         <font>
            <Font name="System Bold Italic" size="19.0" />
         </font></Label>
      <Label layoutX="76.0" layoutY="79.0" text="Username" />
      <Label layoutX="78.0" layoutY="130.0" text="Password" />
      <Label fx:id="error" layoutX="81.0" layoutY="209.0" prefHeight="17.0" prefWidth="149.0" textFill="RED" />
   </children>
</AnchorPane>
