<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AdminSettingsController">
   <children>
      <Label layoutX="23.0" layoutY="23.0" text="Manage Users" />
      <Button fx:id="createNewUser" layoutX="25.0" layoutY="219.0" mnemonicParsing="false" onAction="#createNewUserClicked" text="Create New User" />
      <Button fx:id="back" layoutX="525.0" layoutY="351.0" mnemonicParsing="false" onAction="#backClicked" text="Back" />
      <CheckBox fx:id="isAdminField" layoutX="25.0" layoutY="192.0" mnemonicParsing="false" text="CheckBox" />
      <TextField fx:id="usernameField" layoutX="25.0" layoutY="127.0" />
      <TextField fx:id="passwordField" layoutX="25.0" layoutY="159.0" />
      <Label layoutX="23.0" layoutY="87.0" text="Create New User:" />
      <TableView layoutX="269.0" layoutY="48.0" prefHeight="247.0" prefWidth="308.0">
        <columns>
          <TableColumn fx:id="usernameColumn" prefWidth="75.0" text="username" />
          <TableColumn fx:id="isAdminColumn" prefWidth="75.0" text="isAdmin" />
        </columns>
      </TableView>
      <Label layoutX="272.0" layoutY="23.0" text="Existing Users" />
      <Label fx:id="error" layoutX="23.0" layoutY="252.0" textFill="#fc0000" />
   </children>
</AnchorPane>
