<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.Util.EditDistributorController"
            prefHeight="620.0" prefWidth="720.0"
            styleClass="card" stylesheets="@../CSS/DarkTheme.css">
    <children>
        <Button fx:id="submit" layoutX="486.0" layoutY="325.0" mnemonicParsing="false" onAction="#submit" prefHeight="40.0" prefWidth="84.0" styleClass="button-raised" text="Save" />
        <TextField fx:id="distributorField" layoutX="63.0" layoutY="113.0" />
        <Label fx:id="error" layoutX="35.0" layoutY="329.0" prefHeight="17.0" prefWidth="354.0" />
        <Button fx:id="back" layoutX="423.0" layoutY="325.0" mnemonicParsing="false" onAction="#backClicked" styleClass="button-raised" text="Back" />
        <Label layoutX="63.0" layoutY="64.0" styleClass="card-title" text="Edit Distributor" />
    </children>
</AnchorPane>
